{
  "name": "cartridge-slot-sdk",
  "version": "0.0.0",
  "description": "TypeScript client for Cartridge Slot GraphQL API",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "download-schema": "node scripts/download-schema.js",
    "generate": "pnpm download-schema && graphql-codegen --config codegen.ts",
    "build": "pnpm generate && tsc",
    "dev": "tsc --watch",
    "prepare": "pnpm generate && pnpm build",
    "test": "ts-node --project tsconfig.node.json examples/simple-test.ts",
    "example": "ts-node --project tsconfig.node.json examples/usage.ts"
  },
  "keywords": [
    "dojo",
    "cartridge",
    "slot",
    "graphql"
  ],
	"files": [
		"dist"
	],
  "author": "Cartridge",
  "license": "MIT",
  "devDependencies": {
    "@graphql-codegen/cli": "^5.0.0",
    "@graphql-codegen/client-preset": "^4.1.0",
    "@graphql-codegen/schema-ast": "^5.0.0",
    "@graphql-codegen/typescript": "^4.0.1",
    "@graphql-codegen/typescript-graphql-request": "^6.0.1",
    "@graphql-codegen/typescript-operations": "^4.0.1",
    "@graphql-tools/graphql-file-loader": "^8.1.2",
    "@graphql-tools/url-loader": "^9.0.0",
    "@types/node": "^20.10.0",
    "semantic-release": "^24.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.0"
  },
  "dependencies": {
    "@graphql-typed-document-node/core": "^3.2.0",
    "graphql": "^16.8.1",
    "graphql-request": "^6.1.0",
    "graphql-tag": "^2.12.6"
  }
}
